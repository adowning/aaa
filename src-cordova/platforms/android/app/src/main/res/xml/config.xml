<?xml version='1.0' encoding='utf-8'?>
<widget id="org.cordova.andrews.app" version="v0.1.02" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="Battery">
        <param name="android-package" value="org.apache.cordova.batterystatus.BatteryListener" />
    </feature>
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
    <feature name="BackgroundGeolocation">
        <param name="android-package" value="com.tenforwardconsulting.cordova.BackgroundGeolocationPlugin" />
    </feature>
    <name>andrews-admin</name>
    <description>Administration app for Andrews</description>
    <author email="ash@ashdevtools.com" href="http://ashdevtools.com">
        Apache Cordova Team
    </author>
    <content src="http://192.168.1.8:8080/index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="http://192.168.1.8:8080/index.html" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
