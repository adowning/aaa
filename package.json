{
  "name": "andrews-admin",
  "version": "v0.1.02",
  "description": "Administration app for Andrews",
  "productName": "andrews-admin",
  "cordovaId": "org.cordova.andrews.app",
  "author": "adowning <ashdowning@gmail.com>",
  "main": "src-electron/electron-main.js",
  "private": false,
  "repository": "https://github.com/adowning/andrews-admin.git",
  "build": {
    "squirrelWindows": {
      "remoteReleases": "http://localhost:4567/dist/win1/"
    },
    "win": {
      "certificateFile": "./certs/my_signing_key.pfx",
      "certificatePassword": ""
    }
  },
  "appId": "com.andrews.adminapp",
  "mac": {
    "target": [
      "zip"
    ]
  },
  "win": {
    "target": "squirrel"
  },
  "squirrelWindows": {
    "useAppIdAsId": true
  },
  "scripts": {
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "dist": "electron-builder",
    "publish": "build -p always"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "electron-config": "^1.0.0",
    "electron-updater": "^2.21.0",
    "firebase": "^4.10.1",
    "firebase-functions": "^0.8.1",
    "lowdash": "^1.2.0",
    "vue-browser-geolocation": "^1.0.1",
    "vuelidate": "^0.6.2",
    "vuetify": "^1.0.4",
    "vuex-geolocation": "^1.1.0",
    "vuex-persist": "^1.1.3"
  },
  "devDependencies": {
    "devtron": "1.4.0",
    "electron": "1.7.9",
    "electron-builder": "^20.2.1",
    "electron-debug": "1.4.0",
    "electron-devtools-installer": "2.2.1",
    "electron-packager": "9.1.0",
    "quasar-cli": "^0.15.6"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
